{
    "AWSEBDockerrunVersion": "3",
    "containerDefinitions": [
        {
            "name": "client",
            "image": "wadiukarunathilaka/multi-client-10-14",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "wadiukarunathilaka/multi-server-10-14",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "wadiukarunathilaka/multi-worker-10-14",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "wadiukarunathilaka/multi-nginx-10-14",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"]
        }
    ]
}